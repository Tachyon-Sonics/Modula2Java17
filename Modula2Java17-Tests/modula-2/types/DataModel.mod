MODULE DataModel;

	VAR
		si1, si2: SHORTINT;
		sc1, sc2: SHORTCARD;
		i1, i2: INTEGER;
		c1, c2: CARDINAL;
		li1, li2: LONGINT;
		lc1, lc2: LONGCARD;
		
BEGIN

	si1 := 10;
	si2 := -4;
	si1 := si1 * si2;
	
	sc1 := 7;
	sc2 := 9;
	sc1 := sc1 * sc2;

	i1 := 10;
	i2 := -4;
	i1 := i1 * i2;
	
	c1 := 7;
	c2 := 9;
	c1 := c1 * c2;

	li1 := 10;
	li2 := -4;
	li1 := li1 * li2;
	
	lc1 := 7;
	lc2 := 9;
	lc1 := lc1 * lc2;

END DataModel.